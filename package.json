{
  "name": "between.js",
  "version": "0.0.1-alpha.1",
  "description": "Interpolate between values",
  "main": "./build/between.js",
  "scripts": {
    "test": "xo ./src/**/*.js && ava test/*.spec.js --no-cache",
    "coverage": "nyc npm test | coveralls",
    "build": "cross-env NODE_ENV=production rollup -c && gulp scss:build",
    "start": "cross-env NODE_ENV=development gulp scss:watch & rollup -c -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sasha240100/between.js.git"
  },
  "keywords": [
    "tween",
    "tweening",
    "between",
    "interpolate",
    "transition"
  ],
  "author": "Alexander Buzin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sasha240100/between.js/issues"
  },
  "homepage": "https://github.com/sasha240100/between.js#readme",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.46",
    "@babel/core": "^7.0.0-beta.46",
    "@babel/plugin-transform-modules-commonjs": "^7.0.0-beta.47",
    "@babel/preset-env": "^7.0.0-beta.46",
    "@babel/register": "^7.0.0-beta.46",
    "ava": "^1.0.0-beta.4",
    "babel-plugin-istanbul": "^4.1.6",
    "coveralls": "^3.0.1",
    "cross-env": "^5.1.5",
    "eslint-config-alexander_buzin": "^1.0.2",
    "gulp": "^3.9.1",
    "gulp-sass": "^4.0.1",
    "node-sass": "^4.9.0",
    "nyc": "^11.8.0",
    "once": "^1.4.0",
    "rollup": "^0.58.2",
    "rollup-plugin-babel": "^4.0.0-beta.4",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-serve": "^0.4.2",
    "rollup-watch": "^4.3.1",
    "xo": "^0.21.0"
  },
  "dependencies": {
    "lerp": "^1.0.3",
    "minivents": "^2.2.0"
  },
  "ava": {
    "require": [
      "@babel/register"
    ]
  },
  "xo": {
    "space": 2,
    "esnext": true,
    "parser": "babel-eslint",
    "envs": [
      "node",
      "browser",
      "es6"
    ],
    "syntax_map": {
      "JavaScript (Babel)": "javascript"
    },
    "rules": {
      "operator-linebreak": [
        2,
        "before"
      ],
      "curly": [
        2,
        "multi-or-nest"
      ],
      "one-var": 0,
      "no-else-return": 1,
      "capitalized-comments": 0,
      "no-undef": 0,
      "no-new": 0,
      "camelcase": 0,
      "linebreak-style": 0,
      "one-var-declaration-per-line": 0,
      "new-cap": [
        1,
        {
          "newIsCap": true,
          "capIsNew": true,
          "properties": false
        }
      ],
      "babel/arrow-parens": 0,
      "xo/filename-case": 0,
      "no-warning-comments": 0,
      "unicorn/filename-case": 0,
      "unicorn/no-abusive-eslint-disable": 0,
      "import/no-unassigned-import": 0,
      "import/prefer-default-export": 0,
      "import/extensions": [
        2,
        "never",
        {
          "json": "always"
        }
      ]
    }
  }
}
